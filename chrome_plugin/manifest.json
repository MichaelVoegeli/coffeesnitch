{
  "manifest_version": 2,

  "name": "FNT Coffee pot tracking plugin",
  "description": "This extension will track the brewing progress of your coffee pot",
  "version": "0.0.1.2",
    
  "background": {
    "scripts": ["background.js"]
  },
    
  "browser_action": {
   "default_icon": { 
       "19":"icons/ready/ready19x.png"
   },
   "default_popup": "popup.html"
  },
  "permissions": [
      "activeTab"
   ]
}